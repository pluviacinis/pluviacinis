import matplotlib.pyplot as plt
data = open('chromo.txt', 'r').readlines()
x=[]
y=[]
n=len(data)
for i in range(1,n):
    k=data[i].split()
    x.append(float(k[0]))
    y.append(float(k[1]))
sp1=plt.subplot(111)
sp1.plot(x,y)
plt.title('Chromatogramm')
plt.xlabel('Time')
plt.ylabel('Signal')
plt.show()
